
public class HangSo {

	public static final float PI = 3.14f;//chính tắc
	public static final String v = " String vẫn được !!";//:v
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		final int CONST = 1;// sao vẫn được, tuyệt !
		System.out.println("Hằng số " + (CONST + 1));// nó sẽ là 11 nếu không có ngoặc
		
		float c = PI*6;
		System.out.println(c);
		System.out.println(v);
	}

}
