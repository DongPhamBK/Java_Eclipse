package inner;

public class Animal {
	// inner
	public class Cat {
		private int t;

		public int getT() {
			return t;
		}

		public void setT(int t) {
			this.t = t;
		}

		public void hello() {
			System.out.println("Gau Gau !!!");
		}
	}

	public static void main(String[] args) {
		Animal miu = new Animal();// ok, khai báo kiểu Animal
		Animal.Cat = miu.new Cat();
	}
}
