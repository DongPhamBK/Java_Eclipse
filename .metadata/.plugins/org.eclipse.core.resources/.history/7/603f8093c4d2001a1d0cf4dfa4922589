package OOP;

public class Static {

	static int a = 10;
	
	public static int Example(int x)
	{
		return x*x;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		System.out.println(Static.a);
		Static.a++;
		System.out.println(Static.a);
		
		Static e1 = new Static();

	}

}
