package caudieukien;
import java.util.*;
public class IfElse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner nhap =  new Scanner(System.in);
		
		System.out.println("Nháº­p x:");
		int x = nhap.nextInt();
		
		if ( x!= 10)
			System.out.println("x != 10");
		if(x > 10)
			System.out.println("x > 10");
		else
			System.out.println("x < 10");
	}

}
